generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum {
  
}

model User {
  id       Int      @id @default(autoincrement())
  name     String
  email    String   @unique
  password String
  created  DateTime @default(now())
}

model pdfTable {
  id         Int      @id @default(autoincrement())
  fileName   String   @unique
  fileSize   String
  pdf        String
  category   String
  uploadedAt DateTime @default(now())
}
